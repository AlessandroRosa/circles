external_def:draw_complex_disk( _context, _mapper, _complex_x, _complex_y, _complex_radius, _draw, _drawcolor, _fill, _fillcolor, _linewidth, _opacity, _sector_rad_start, _sector_rad_end )
internal_def:circles_lib_draw_complex_disk( _context, _mapper, _complex_x, _complex_y, _complex_radius, _draw, _drawcolor, _fill, _fillcolor, _linewidth, _opacity, _sector_rad_start, _sector_rad_end )
